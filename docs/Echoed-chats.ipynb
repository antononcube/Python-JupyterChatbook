      "import random\n",
      "\n",
      "def random_walk(n):\n",
      "    x, y = 0, 0\n",
      "    for _ in range(n):\n",
      "        dx, dy = random.choice([(0, 1), (0, -1), (1, 0), (-1, 0)])\n",
      "        x += dx\n",
      "        y += dy\n",
      "    return x, y\n"
     ]
    }
